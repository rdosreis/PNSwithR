@article{malta_surveillance_2015,
	title = {Surveillance and monitoring of major chronic diseases in {Brazil} - {National} {Health} {Survey}, 2013},
	volume = {18},
	issn = {1415-790X, 1980-5497},
	url = {http://www.scielo.br/j/rbepid/a/wxBbmB3rBkZy3BcYTtq5Spk/?lang=en},
	doi = {10.1590/1980-5497201500060002},
	abstract = {RESUMO: Objetivo: Descrever as principais doenças crônicas não transmissíveis (DCNT) no país segundo as informações coletadas em indivíduos de 18 anos ou mais de idade. Métodos: Foram utilizados dados da Pesquisa Nacional de Saúde (PNS), 2013, estudo transversal de base populacional. As proporções de cada DCNT foram calculadas e apresentadas segundo sexo, com intervalo de confiança de 95\% (IC95\%), com os valores absolutos. Resultados: Do total de entrevistados, 45,1\% referiram ter pelo menos uma DCNT. A Região com maior prevalência de DCNT foi a Sul (52,1\%). A hipertensão arterial apresentou a maior prevalência dentre as DCNT, com 21,4\%, seguida por problema crônico de coluna (18,5\%), depressão (7,6\%), artrite (6,4\%) e diabetes (6,2\%). O grau de limitação intenso/muito intenso apresentou maiores prevalências para outra doença mental (37,6\%) e acidente vascular cerebral (AVC) (25,5\%). Conclusão: A melhoria dos serviços de saúde é indispensável para uma resposta efetiva à dupla carga de adoecimento de países de média e baixa renda.},
	language = {en},
	urldate = {2022-09-21},
	journal = {Revista Brasileira de Epidemiologia},
	author = {Malta, Deborah Carvalho and Stopa, Sheila Rizzato and Szwarcwald, Celia Landmann and Gomes, Nayara Lopes and Silva Júnior, Jarbas Barbosa and Reis, Ademar Arthur Chioro dos},
	month = dec,
	year = {2015},
	note = {Publisher: Associação Brasileira de Saúde Coletiva},
	keywords = {Diabetes mellitus, Health surveys, Hypertension, Chronic disease, Epidemiological surveillance., Low back pain},
	pages = {03--16},
	file = {Malta et al_2015_Surveillance and monitoring of major chronic diseases in Brazil - National.pdf:C\:\\Users\\rodri\\Zotero\\storage\\K39MAHZI\\Malta et al_2015_Surveillance and monitoring of major chronic diseases in Brazil - National.pdf:application/pdf;Snapshot:C\:\\Users\\rodri\\Zotero\\storage\\5IBN3JVA\\wxBbmB3rBkZy3BcYTtq5Spk.html:text/html},
}

@article{malta_prevalence_2019,
	title = {Prevalence of altered total cholesterol and fractions in the {Brazilian} adult population: {National} {Health} {Survey}},
	volume = {22},
	issn = {1415-790X, 1980-5497},
	shorttitle = {Prevalence of altered total cholesterol and fractions in the {Brazilian} adult population},
	url = {http://www.scielo.br/j/rbepid/a/gxFK6KvfqFRPWJxwJKmhFqq/?lang=en},
	doi = {10.1590/1980-549720190005.supl.2},
	abstract = {RESUMO: Objetivo: Analisar as prevalências dos níveis de colesterol total e frações alterados na população brasileira, segundo dados bioquímicos da Pesquisa Nacional de Saúde. Métodos: Estudo descritivo, utilizando dados laboratoriais da Pesquisa Nacional de Saúde coletados entre os anos de 2014 e 2015. Foram analisados exames de colesterol total e frações e calculadas prevalências populacionais de valores alterados segundo variáveis sociodemográficas. Consideraram-se os seguintes pontos de corte: colesterol total ≥ 200mg/dL; lipoproteínas de baixa densidade (LDL) ≥ 130mg/dL e lipoproteínas de alta densidade (HDL) {\textless} 40mg/dL. Resultados: Aprevalência de colesterol total ≥ 200mg/dL na população foi de 32,7\%, mais elevada em mulheres (35,1\%). A prevalência de HDL alterado foi de 31,8\%, sendo de 42,8\% no sexo masculino e 22,0\% no feminino. LDL≥ 130mg/dL foi observado em 18,6\%, com prevalência mais elevada em mulheres (19,9\%). População com idade de 45 anos ou mais e com baixa escolaridade apresentou maiores prevalências de colesterol com alterações. Conclusão: Valores de colesterol total e frações alterados foram frequentes na população brasileira, especialmente entre mulheres, idosos e pessoas de baixa escolaridade. Esses resultados poderão orientar as ações de controle e prevenção, como alimentação saudável, atividade física e tratamento, visando à prevenção de doenças coronarianas.},
	language = {en},
	urldate = {2022-09-21},
	journal = {Revista Brasileira de Epidemiologia},
	author = {Malta, Deborah Carvalho and Szwarcwald, Celia Landman and Machado, Ísis Eloah and Pereira, Cimar Azeredo and Figueiredo, André Willian and Sá, Ana Carolina Micheletti Gomide Nogueira de and Velasquez-Melendez, Gustavo and Santos, Filipe Malta dos and Souza, Paulo Borges de and Stopa, Sheila Rizzato and Rosenfeld, Luiz Gastão},
	month = oct,
	year = {2019},
	note = {Publisher: Associação Brasileira de Saúde Coletiva},
	keywords = {Health surveys, Cholesterol, Cardiovascular diseases, HDL, Laboratory test, LDL},
	file = {Malta et al_2019_Prevalence of altered total cholesterol and fractions in the Brazilian adult.pdf:C\:\\Users\\rodri\\Zotero\\storage\\J56VYZH6\\Malta et al_2019_Prevalence of altered total cholesterol and fractions in the Brazilian adult.pdf:application/pdf;Snapshot:C\:\\Users\\rodri\\Zotero\\storage\\26ZVAMXL\\gxFK6KvfqFRPWJxwJKmhFqq.html:text/html},
}

@article{malta_evaluation_2019,
	title = {Evaluation of renal function in the {Brazilian} adult population, according to laboratory criteria from the {National} {Health} {Survey}},
	volume = {22},
	issn = {1415-790X, 1980-5497},
	url = {http://www.scielo.br/j/rbepid/a/vCRTpQR5Xdx6fH9tKRB4vmn/?lang=en},
	doi = {10.1590/1980-549720190010.supl.2},
	abstract = {RESUMO: Objetivo: O presente estudo avaliou a função renal da população adulta brasileira, segundo critérios laboratoriais da Pesquisa Nacional de Saúde (PNS). Metodologia: Estudo descritivo realizado com os dados laboratoriais da PNS, coletados entre os anos de 2014 e 2015. Com base nos dados laboratoriais foram analisadas prevalências populacionais de creatinina sérica (CR) e estimativa da taxa de filtração glomerular (TFG), segundo variáveis sociodemográficas. Resultados: A amostra foi de 8.535 indivíduos com idade de 18 anos ou mais para o estudo da CR e de 7.457 indivíduos para o estudo de TFG. A prevalência TFG {\textless} 60 mL/min/1,73 m2 foi de 6,7\% (IC95\% 6,0 - 7,4), foi mais elevada em mulheres (8,2\% IC95\% 7,2 - 9,2) do que em homens (5,0\% IC95\% 4,2 - 6,0) p {\textless} 0,001 e em idosos ≥ 60 anos foi de 21,4\%. Os valores de CR ≥ 1,3 mg/dL em homens foram 5,5\% (IC95\% 4,6 - 6,5) e em mulheres foram de CR ≥ 1,1 mg/dL, de 4,6\% (IC95\% 4,0- 5,4), sem diferença estatística significativa nos valores de CR entre sexo, p = 0,140. Conclusão: Resultados laboratoriais da PNS identificaram prevalências mais elevadas da doença renal crônica na população brasileira do que o estimado em estudos autorreferidos. ATFG {\textless} 60 mL/min/1,73 m2 é mais elevada em mulheres e atinge um quinto dos idosos. Esses exames podem ser úteis no propósito de identificar precocemente a doença e, dessa forma, prevenir a progressão da lesão renal e reduzir o risco de eventos cardiovasculares e de mortalidade.},
	language = {en},
	urldate = {2022-09-21},
	journal = {Revista Brasileira de Epidemiologia},
	author = {Malta, Deborah Carvalho and Machado, Ísis Eloah and Pereira, Cimar Azeredo and Figueiredo, André Willian and Aguiar, Lilian Kelen de and Almeida, Wanessa da Silva de and Souza, Maria de Fatima Marinho de and Rosenfeld, Luiz Gastão and Szwarcwald, Célia Landman},
	month = oct,
	year = {2019},
	note = {Publisher: Associação Brasileira de Saúde Coletiva},
	keywords = {Noncommunicable diseases, Risk factors, Chronic renal insufficiency, Creatinine, Glomerular filtration rate, Health survey},
	file = {Malta et al_2019_Evaluation of renal function in the Brazilian adult population, according to.pdf:C\:\\Users\\rodri\\Zotero\\storage\\JRLGBFBU\\Malta et al_2019_Evaluation of renal function in the Brazilian adult population, according to.pdf:application/pdf;Snapshot:C\:\\Users\\rodri\\Zotero\\storage\\FILW7IKB\\vCRTpQR5Xdx6fH9tKRB4vmn.html:text/html},
}

@article{malta_prevalence_2019-1,
	title = {Prevalence of diabetes mellitus as determined by glycated hemoglobin in the {Brazilian} adult population, {National} {Health} {Survey}},
	volume = {22},
	issn = {1415-790X, 1980-5497},
	url = {http://www.scielo.br/j/rbepid/a/qQttB6XwmqzJYgcZKfpMV7L/?lang=en},
	doi = {10.1590/1980-549720190006.supl.2},
	abstract = {RESUMO: Objetivo: Analisar as prevalências de diabetes mellitus segundo diferentes critérios diagnósticos, na população adulta brasileira, segundo os resultados laboratoriais da Pesquisa Nacional de Saúde. Métodos: Análise dos dados laboratoriais da Pesquisa Nacional de Saúde, coletados entre os anos de 2014 e 2015. Foram calculadas as prevalências de diabetes conforme diferentes critérios diagnósticos. Foram calculadas as prevalências de diabetes segundo o critério de hemoglobina glicosilada ≥ 6,5\% ou em uso de medicamentos, empregando regressão de Poisson para o cálculo da razão de prevalência (RP) bruta e ajustada e intervalo de confiança de 95\% (IC95\%). Resultados: A prevalência de diabetes segundo diferentes critérios pode variar 6,6 a 9,4\%; e a hiperglicemia intermediária, ou pré-diabetes, de 6,8 a 16,9\%. Usando-se o critério laboratorial ou uso de medicamentos, a prevalência de diabetes foi de 8,4\%. A RP ajustada para sexo, idade, escolaridade e região foi menor no sexo masculino (RP = 0,75; IC95\% 0,63 - 0,89); aumentou com a idade: 30 a 34 anos (RP=2,32; IC95\% 1,33 - 4,07), 40 a 59 anos (RP = 8,1; IC95\% 4,86 - 13,46), 60 anos ou mais (RP = 12,6; IC95\% 7,1 - 21,0); e a escolaridade elevada foi protetora (RP = 0,8; IC95\% 0,6 - 0,9). Maior RP foi encontrada na Região Centro-Oeste (RP = 1,3; IC95\% 1,04 - 1,7) e naqueles com sobrepeso (RP = 1,8; IC95\% 1,4 - 2,1) e obesidade (RP = 3,3; IC95\% 2,6 - 4,1). Conclusão: A prevalência de diabetes foi maior no sexo feminino, naqueles com idade maior que 30 anos, em população com baixa escolaridade, com excesso de peso e obesidade. Os critérios laboratoriais são mais fidedignos para o conhecimento da situação real do diabetes no país.},
	language = {en},
	urldate = {2022-09-21},
	journal = {Revista Brasileira de Epidemiologia},
	author = {Malta, Deborah Carvalho and Duncan, Bruce Bartholow and Schmidt, Maria Inês and Machado, Ísis Eloah and Silva, Alanna Gomes da and Bernal, Regina Tomie Ivata and Pereira, Cimar Azeredo and Damacena, Giseli Nogueira and Stopa, Sheila Rizzato and Rosenfeld, Luiz Gastão and Szwarcwald, Celia Landman},
	month = oct,
	year = {2019},
	note = {Publisher: Associação Brasileira de Saúde Coletiva},
	keywords = {Diabetes mellitus, Clinical laboratory techniques, Glycated hemoglobin A, Noncommunicable diseases, Obesity, Prediabetic state},
	file = {Malta et al_2019_Prevalence of diabetes mellitus as determined by glycated hemoglobin in the.pdf:C\:\\Users\\rodri\\Zotero\\storage\\PZEZWFYU\\Malta et al_2019_Prevalence of diabetes mellitus as determined by glycated hemoglobin in the.pdf:application/pdf;Snapshot:C\:\\Users\\rodri\\Zotero\\storage\\BEPTBGXA\\qQttB6XwmqzJYgcZKfpMV7L.html:text/html},
}

