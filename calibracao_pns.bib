@article{dos_reis_control_2021,
	title = {Control of {Glucose}, {Blood} {Pressure}, and {Cholesterol} among {Adults} with {Diabetes}: {The} {Brazilian} {National} {Health} {Survey}},
	volume = {10},
	copyright = {http://creativecommons.org/licenses/by/3.0/},
	issn = {2077-0383},
	shorttitle = {Control of {Glucose}, {Blood} {Pressure}, and {Cholesterol} among {Adults} with {Diabetes}},
	url = {https://www.mdpi.com/2077-0383/10/15/3428},
	doi = {10.3390/jcm10153428},
	abstract = {ABC (glucose, blood pressure and LDL-cholesterol) goals are basic standards of diabetes care. We aimed to assess ABC control and related factors in a representative sample of Brazilian adults with diabetes. We analyzed 465 adults with known diabetes in the Brazilian National Health Survey. The targets used were {\textless}7\% for glycated hemoglobin (A1C); {\textless}140/90 mmHg for blood pressure; and {\textless}100 mg/dL for LDL-C, with stricter targets for the latter two for those with high cardiovascular (CVD) risk. Individual goals were attained by 46\% (95\% CI, 40.3–51.6\%) for A1C, 51.4\% (95\% CI, 45.7–57.1\%) for blood pressure, and 40\% (95\% CI, 34.5–45.6\%) for LDL-C. The achievement of all three goals was attained by 12.5\% (95\% CI, 8.9–16.2\%). Those with high CVD risk attained blood pressure and LDL-C goals less frequently. A1C control improved with increasing age and worsened with greater duration of diabetes. Achievement of at least two ABC goals decreased with increasing BMI and greater duration of diabetes. In sum, about half of those with known diabetes achieved each ABC goal and only a small fraction achieved all three goals. Better access and adherence to treatment and strategies to personalize goals according to specific priorities are of the essence.},
	language = {en},
	number = {15},
	urldate = {2022-09-21},
	journal = {Journal of Clinical Medicine},
	author = {dos Reis, Rodrigo Citton P. and Duncan, Bruce B. and Szwarcwald, Célia Landmann and Malta, Deborah Carvalho and Schmidt, Maria Inês},
	month = jan,
	year = {2021},
	note = {Number: 15
Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {diabetes mellitus, blood pressure, LDL cholesterol, glucose control, measuring quality of diabetes care},
	pages = {3428},
	file = {dos Reis et al_2021_Control of Glucose, Blood Pressure, and Cholesterol among Adults with Diabetes.pdf:C\:\\Users\\rodri\\Zotero\\storage\\T77WDRAY\\dos Reis et al_2021_Control of Glucose, Blood Pressure, and Cholesterol among Adults with Diabetes.pdf:application/pdf;Snapshot:C\:\\Users\\rodri\\Zotero\\storage\\TM3W6G8H\\htm.html:text/html},
}

